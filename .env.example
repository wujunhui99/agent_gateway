# OpenIM API Configuration
AGENT_GATEWAY_OPENIM_API_BASE=http://127.0.0.1:10002
AGENT_GATEWAY_OPENIM_ADMIN_USER_ID=openIMAdmin
AGENT_GATEWAY_OPENIM_ADMIN_SECRET=openIMAdminSecret

# Gateway Server Configuration
AGENT_GATEWAY_HOST=0.0.0.0
AGENT_GATEWAY_PORT=8081

# Agent Configuration
AGENT_GATEWAY_AGENT_USER_PREFIX=bot_

# Redis (optional)
AGENT_GATEWAY_REDIS_URL=redis://localhost:6379/0

# MongoDB (agent metadata)
AGENT_GATEWAY_MONGO_URI=mongodb://root:rootpass@127.0.0.1:47017
AGENT_GATEWAY_MONGO_DB=agent_gateway
AGENT_GATEWAY_MONGO_AGENT_COLLECTION=agents

# LLM Provider Configuration
# Aliyun
AGENT_GATEWAY_LLM_ALIYUN_API_BASE=https://dashscope.aliyuncs.com/compatible-mode/v1
AGENT_GATEWAY_LLM_ALIYUN_API_KEY=your_aliyun_api_key

# SiliconFlow
AGENT_GATEWAY_LLM_SILICONFLOW_API_BASE=https://api.siliconflow.cn/v1
AGENT_GATEWAY_LLM_SILICONFLOW_API_KEY=your_siliconflow_api_key
